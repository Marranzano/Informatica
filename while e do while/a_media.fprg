            </do>
            <assign variable="media" expression="sommanum /volte"/>
            <output expression="&quot;la somma e'&quot;" newline="True"/>
            <output expression="sommanum" newline="True"/>
            <output expression="&quot;la media e'&quot;" newline="True"/>
            <output expression="media" newline="True"/>
        </body>
