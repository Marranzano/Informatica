    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="numero, volte, sommanum" type="Integer" array="False" size=""/>
            <declare name="media" type="Real" array="False" size=""/>
            <assign variable="numero" expression="0"/>
            <assign variable="volte" expression="-1"/>
